{
  "name": "react-template",
  "version": "1.0.0",
  "description": "starting template for react projects",
  "main": "src/app.js",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "start": "NODE_ENV=development webpack-dev-server",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:js": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:types": "tsc --noEmit --skipLibCheck",
    "lint": "run-p lint:*",
    "format:js": "prettier --write 'src/**/*.{js,jsx,ts,tsx}' && eslint --fix 'src/**/*.{js,jsx,ts,tsx}' --config .eslintrc-format.js",
    "format:css": "prettier --write 'src/**/*.css' && stylelint --fix 'src/**/*.css' --config .stylelintrc-format.js",
    "format": "run-p format:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v1z/react-template.git"
  },
  "author": "v1z",
  "license": "MIT",
  "homepage": "https://github.com/v1z/react-template#readme",
  "lint-staged": {
    "*.css": [
      "prettier --write",
      "stylelint --fix --config .stylelintrc-format.js",
      "git add"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix --config .eslintrc-format.js",
      "git add"
    ],
    "*.{md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.1",
    "@mui/material": "^6.1.1",
    "@reduxjs/toolkit": "^2.2.7",
    "axios": "^1.7.7",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "ethers": "^5.4",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "smoothscroll": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^29.5.2",
    "@types/jest-expect-message": "^1.1.0",
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^3.2.0",
    "eslint": "^8.14.0",
    "eslint-plugin-codeceptjs": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^3.0.5",
    "image-webpack-loader": "^6.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-expect-message": "^1.1.3",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^2.9.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^3.3.3",
    "style-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylelint-at-rule-no-children": "^0.3.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-media-use-custom-media": "^1.0.0",
    "stylelint-order": "^3.1.0",
    "stylelint-value-no-unknown-custom-properties": "^2.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^4.7.4",
    "url-loader": "^2.1.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
